        {
          "type": "ReferenceGroup",
          "primaryrefs": [],
          "secondaryrefs": [
            {
              "type": "Reference",
              "refclass": "B",
              "comment": "The ungodliness of the men of Sodom lay in sexual immorality",
              "continuation": false,
              "bcvreferences": [
                "01019004-01019005"
              ],
              "parallels": []
            },
            {
              "type": "Reference",
              "refclass": "B",
