        },
        {
          "type": "CommentSubheadingTitle",
          "title": "The name is not to be misused",
          "precomment": "",
          "refgroups": [
            {
              "type": "ReferenceGroup",
              "primaryrefs": [
                {
                  "type": "Reference",
                  "refclass": "A",
                  "comment": "The misuse referred to here is swearing falsely by God's name.",
                  "continuation": false,
                  "bcvreferences": [
                    "02020007"
                  ],
                  "parallels": [
                    "05005011"
                  ]
                }
              ],
              "secondaryrefs": [],
              "seealsos": [
                {
                  "type": "Reference",
                  "refclass": "B",
                  "comment": "The Jews came to believe that just uttering the name ``Yahweh'' was an offence and as a result its exact pronunciation has been lost. What merited the death penalty here was using God's name as a curse.",
                  "continuation": false,
                  "bcvreferences": [
                    "03019012",
                    "03024011",
                    "03024015-03024016"
                  ],
                  "parallels": []
                }
              ]
            }
          ]
        }
